<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.sprout.frame.app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".DBSimpleActivity"/>
        <activity android:name=".NetSimpleActivity"/>
        <activity android:name=".PermissionSimpleActivity"/>
        <activity android:name=".CaptureSimpleActivity"/>
        <activity android:name=".WebViewSimpleActivity"/>
    </application>

</manifest>